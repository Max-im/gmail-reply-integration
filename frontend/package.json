{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.18.0",
    "chart.js": "^2.7.2",
    "jwt-decode": "^2.2.0",
    "node-async-loop": "^1.2.2",
    "node-sass": "^4.9.3",
    "npm-run-all": "^4.1.3",
    "random-color": "^1.0.1",
    "react": "^16.4.1",
    "react-chartjs-2": "^2.7.4",
    "react-dom": "^16.4.1",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.4",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && npm run build-js",
    "start-js": "react-scripts start",
    "build-js": "react-scripts build",
    "build-css": "node-sass src/scss -o src/scss",
    "watch-css": "npm run build-css && node-sass src/scss -o src/scss --watch --recursive",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "proxy": "http://localhost:5000"
}
